{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "decimal"
    },
    "quantity": {
      "type": "integer"
    },
    "invoiceItems": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::invoice-item.invoice-item",
      "mappedBy": "product"
    },
    "image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    }
  }
}
